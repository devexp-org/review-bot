    {
      "test": "\/add (@\\w+)",
      "handlers": ["command-add"]
    },
    {
      "test": "\/change (@\\w+) to (@\\w+)",
      "handlers": ["command-change"]
    },
    {
      "test": "\/remove (@\\w+)",
      "handlers": ["command-remove"]
    },
    {
      "test": "\/replace (@\\w+)",
      "handlers": ["command-replace"]
