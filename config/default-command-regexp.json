    {
      "test": "^пинг",
      "handlers": ["command-ping"]
    },
    {
      "test": "\/ok",
      "handlers": ["command-ok"]
    },
    {
      "test": "^ok",
      "handlers": ["command-ok"]
    },
    {
      "test": "^lgtm",
      "handlers": ["command-ok"]
    },
    {
      "test": "^:ok:",
      "handlers": ["command-ok"]
    },
    {
      "test": "^:fire:",
      "handlers": ["command-ok"]
    },
    {
      "test": "\/збс",
      "handlers": ["command-ok"]
