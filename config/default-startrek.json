{

  "startrek-update-reviewers": {
    "path": "./src/plugins/startrek/update-reviewers",
    "options": {
      "queues": [
        "FEI",
        "IMAGESUI",
        "ISL",
        "ISLSUGGEST",
        "MOBVIDEO",
        "NERPA",
        "SAKHALIN",
        "SERP",
        "VIDEOUI"
      ],
      "events": [
        "review:command:ok",
        "review:command:add",
        "review:command:busy",
        "review:command:start",
        "review:command:change",
        "review:command:remove"
      ]
    },
    "dependencies": [
      "events",
      "logger",
      "startrek"
    ]
  },

  "startrek-update-status": {
    "path": "./src/plugins/startrek/set-issue-status",
    "options": {
      "queues": [
        "FEI",
        "IMAGESUI",
        "ISL",
        "ISLSUGGEST",
        "MOBVIDEO",
        "NERPA",
        "SAKHALIN",
        "SERP",
        "VIDEOUI"
      ],
      "events": [
        "review:command:start"
      ]
    },
    "dependencies": [
      "events",
      "logger",
      "startrek"
    ]
  },

  "#comment:startrack-set-components": {
    "path": "./src/plugins/startrek/set-components",
    "options": {
      "queues": ["SERP"],
      "events": [
        "review:command:start"
      ]
    },
    "dependencies": [
      "events",
      "logger",
      "startrek",
      "components-api"
    ]
  }

}
